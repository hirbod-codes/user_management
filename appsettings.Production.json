{
    "Logging": {
        "LogLevel": {
            "Default": "Warning",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "Grpc": {
        "Address": "http://localhost:6001"
    },
    "Kestrel": {
        "Endpoints": {
            "Http": {
                "Url": "http://0.0.0.0:5002"
            },
            "Https": {
                "Url": "https://0.0.0.0:5003"
            }
        },
        "EndpointDefaults": {
            "Url": "https://0.0.0.0:5003",
            "Protocols": "Http1"
        }
    },
    "Jwt": {
        "ValidIssuer": "user_management",
        "ValidAudience": "internal"
    },
    "MongoDB": {
        "caPem": "/security/ca.pem",
        "certificateP12": "/security/app.p12",
        "collections": {
            "Users": "users",
            "Clients": "clients"
        }
    }
}